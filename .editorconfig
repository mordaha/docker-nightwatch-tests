[*.js]
indent_style = space
indent_size = 2

[*.py]
indent_style = space
indent_size = 4

[*.conf]
indent_style = space
indent_size = 4

[*.gradle]
indent_style = space
indent_size = 4

[*.txt]
indent_style = tab
indent_size = 8
insert_final_newline = false

[*.xml]
indent_style = space
indent_size = 2

[Fastfile]
indent_style = space
indent_size = 2

[Matchfile]
indent_style = space
indent_size = 2

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
